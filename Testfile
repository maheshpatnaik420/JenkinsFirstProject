node()
{
def projectproperties=null
  stage('firststage')
    {
      echo 'Hi this is my first stage'
      checkout scm
    }

  stage('Readingstage')
    {

      projectproperties = "${env}"

      script {
                    env.keySet().each { key ->
                        echo "${key} = ${env[key]}"
                    }

    }
}

}
